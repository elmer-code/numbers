<!DOCTYPE html>
<html>
<head>
  <title>Number</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">

    <% flash.each do |key, value| %>
      <div class="flash" id="<%= key %>">
        <%= value %>
      </div>
    <% end %>

    <%= yield %>

  </div>
</body>
</html>
